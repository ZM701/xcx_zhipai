/* components/personalPage/personalPage.wxss */
.nav{
  position: relative;
  height: 150px;
}
.nav .bg{
  position: absolute;
  width: 100%;
  height: 100px;
  left: 0;
  top:0;
}
.nav .bg image{
  width: 100%;
  height: 150%;
}
.nav .navback{
  position: absolute;
  height: 25px;
  line-height: 25px;
  width: 90%;
  top:10px;
  left: 50%;
  margin-left: -45%;
}
.nav .navback image{
  float: left;
  width: 24px;
  height: 24px;
}
.nav .navback text{
  float: right;
  padding: 2px 10px;
  background: rgba(255,255,255, 0.4);
  font-size: 14px;
  color: #fff;
  border-radius: 20px;
}
.nav .userInfo{
  position: absolute;
  width: 90%;
  left: 50%;
  margin-left: -45%;
  bottom: 10px;
  overflow: hidden;
  color:#fff;
}
.nav .userInfo .box1,.nav .userInfo .box2{
  float: left;
}
.nav .userInfo .box2{
  width: 50%;
  margin-left: 5px;
  font-size: 14px;
}
.nav .userInfo .box3{
  float: right;
}
.nav .userInfo .box1 image{
  width: 50px;
  height: 50px;
  border-radius: 50%;
}
.nav .userInfo .box2 view:first-of-type image{
  width: 10px;
  height: 10px;
  margin: 0 5px;
}
.nav .userInfo .box2 view:first-of-type text.attention{
  display: inline-block;
  padding: 4px 10px;
  background: rgba(0,0,0,0.5);
  font-size: 12px;
  border-radius: 10px;
}
.nav .userInfo .box2 view:first-of-type{
  margin: 2px 0;
}
.nav .userInfo .box3 text{
  display: block;
  font-size: 14px;
}
.nav .userInfo .box3 text:last-of-type{
  border:1px solid #fff;
  padding: 0px 12px;
  border-radius: 10px;
  font-size: 12px;
  margin-top: 5px;
}
.foorPrint{
  width: 80px;
  padding-bottom: 5px;
  border-bottom: 2px solid #a9e29a;
  margin-left: 25px;
  margin-top: 10px;
  margin-bottom: 5px;
}
.foorPrint text:first-of-type{
  font-size: 14px;
  font-weight: bold;
  color: #303030;
}
.foorPrint text:last-of-type{
  font-size: 11px;
  color: #a5a5a5;
  padding-left: 5px;
}
.main{
  margin-bottom: 10px;
  overflow: hidden;
}
.total{
  padding-left: 5px;
  overflow: hidden;
}
.total .time,.total .box{
  float: left;
  overflow: hidden;
}
.total .time text{
  display: block;
}
.total .time text:first-of-type{
  font-size: 14px;
  color: #565656;
}
.total .time text:last-of-type{
  font-size: 10px;
  color: #9d9d9d;
}
.total .box{
  width: 92%;
}
.total .box .dot{
  width: 10px;
  height: 10px;
  background: #dfdfdf;
  border-radius: 50%;
  margin: 5px 0;
}
.total .box .timeLine{
 width: 98%;
 border-left: 1px solid #dfdfdf; 
 padding: 5px 0px 5px 10px; 
 margin-left: 5px; 
}
.footprint{
  width: 100%;
}
.footprint .img{
  width: 100%;
  height: 197px;
  background: #E5E5E5;
  margin-bottom: 9px;
  position: relative;
}
.footprint .img image{
  position: absolute;
  left: 0;
  top:0;
  width: 100%;
  height: 100%;
}
.footprint .img text.number{
  text-align: center;
  position: absolute;
  right: 15px;
  top: 7px;
  font-size: 11px;
  color: #fff;
  height: 15px;
  width: 25px;
  background: rgba(0, 0, 0, 0.35);
}
.footprint .img text.look{
  position: absolute;
  right: 15px;
  bottom: 7px;
  font-size: 10px;
  color: #fff;
}
.footprint .desc .print view:first-of-type{
  text-align: left;
  font-size: 13px;
  color: #8f8f8f;
  margin-bottom: 8px;
}
.footprint .desc .print view:last-of-type{
  text-align: left;
  color: #8f8f8f;
  font-size: 11px;
}
.footprint .desc {
  overflow: hidden;
}
.footprint .desc .print {
  float: left;
}
.footprint .desc .thums{
  float: right;
  margin-right: 20px;
  /* line-height: 35px; */
}
.footprint .desc .thums image{
  width: 12px;
  height: 11px;
  margin-right: 2px;
}
.footprint .desc .thums text{
  font-size: 11px;
  color: #8f8f8f;
}
.none image{
  width: 100px;
  height: 90px;
  display: block;
  margin: 50px auto 10px;
}
.none{
  text-align:center;
  color: #CECECE;
}

