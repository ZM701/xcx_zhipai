/* pages/gallery/gallery.wxss */

page{ background: #f0f0f0;}
.installList{ margin-top: 14rpx; border-top: 1px solid #e7e7e7;background: #fff;}
.installList .item:last-child{
  border: none;
}
.item{
  display: flex;
  padding:24rpx;
  flex-direction: row;
  justify-content: space-between;  
  position: relative;
  border-bottom: 1px solid #e5e5e5;
  margin-left: 24rpx;  
}
.item>view {
  display: flex;
  font-size: 28rpx;
  color: #252525;
  margin-left: 10rpx;
  height: 50rpx;
  line-height: 50rpx;
}

.addImg{
  width: 200rpx;
  height: 200rpx;
  background: #fff;
  margin: 30rpx 0 0 24rpx;
}

.addImg .iconfont{
  display: block;
  text-align: center;
font-size:60rpx;line-height:200rpx;color:#999;}


