/* pages/gallery/gallery.wxss */

page{ background: #f0f0f0;}
.installList{ margin-top: 14rpx; border-top: 1px solid #e7e7e7;background: #fff;}
.installList .item:last-child{
  border: none;
}
.item{
  width: 100%;
  display: flex;
  padding:24rpx;
  flex-direction: row;
  justify-content: space-between;  
  position: relative;
  border-bottom: 1px solid #e5e5e5;
  margin-left: 24rpx;  
}
.item>view {
  width: 100%;
  display: flex;
  font-size: 28rpx;
  color: #252525;
  margin-left: 10rpx;
  height: 50rpx;
  line-height: 50rpx;
}
.item input{
  width: 80%;
}
.big-logos{
  overflow: hidden;
}
.logoinfo1{
  width: 190rpx;
  height: 190rpx;
  margin: 10px;
  border:1px dashed #999999;
  float: left;
}
.logoinfo1 image{
  display: block;
  width: 100rpx;
  height: 100rpx;
  margin: 50rpx auto;
}
.logoinfo{
  position: relative;
  width: 190rpx;
  height: 190rpx;
  float: left;
  background: #fff;
  margin: 10px;
  border:1px solid #fff;
}
.logoinfo image{
  display: block;
  width: 100%;
  height: 100%;
  position: absolute;
  left: 50%;
  top:50%;
  margin-left: -95rpx;
  margin-top: -95rpx;
}
.logoinfo icon{
  position: absolute;
  right: 2px;
  top:2px;
}
.btn{
  width: 90%;
  margin-top: 70rpx;
  background: #a9e29a !important;
  color: #fff;
  position: fixed;
  bottom: 30rpx;
  left: 5%;
}


